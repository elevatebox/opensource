{
    "manifest_version": 3,
    "name": "Smart Notes & Bookmarks Manager",
    "version": "1.0",
    "description": "Effortlessly manage bookmarks and smart notes.",
    "permissions": ["storage", "bookmarks"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  